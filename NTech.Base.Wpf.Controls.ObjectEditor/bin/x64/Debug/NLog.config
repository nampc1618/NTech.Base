<?xml version="1.0" encoding="utf-8" ?>
<nlog xmlns="http://www.nlog-project.org/schemas/NLog.xsd"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://www.nlog-project.org/schemas/NLog.xsd NLog.xsd"
      autoReload="true"
      throwExceptions="false"
      internalLogLevel="Off" internalLogFile="c:\temp\nlog-internal.log">
  <targets>
    <!--<target name="CCIGUI" xsi:type="File"
            layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message}"
            fileName="C:\KYLOG\AOIGUI\CCIGUI\KYLOG-CCIGUI-${date:yyyyMMdd}.LOG"
            keepFileOpen="false"
            archiveNumbering="Sequence"
            archiveAboveSize="5242880"
            archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\KYLOG-CCIGUI-${date:yyyyMMdd}_{#}.LOG"
            encoding="utf-8" />-->

    <target name="CCIINFO" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\INFO\KYLOG-CCIGUI-INFO-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\INFO\KYLOG-CCIGUI-INFO-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="CCIERROR" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\ERROR\KYLOG-CCIGUI-ERROR-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\ERROR\KYLOG-CCIGUI-ERROR-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="CCIGUICONNECTOR" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\GUICONNECTOR\KYLOG-CCIGUI-GUICONNECTOR-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\GUICONNECTOR\KYLOG-CCIGUI-GUICONNECTOR-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="CCIDBCONNECTOR" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\DBCONNECTOR\KYLOG-CCIGUI-DBCONNECTOR-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\DBCONNECTOR\KYLOG-CCIGUI-DBCONNECTOR-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="CCIDEBUG" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\DEBUG\KYLOG-CCIGUI-DEBUG-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\DEBUG\KYLOG-CCIGUI-DEBUG-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="GLOBALIZATION" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\GLOBALIZATION\KYLOG-GLOBALIZATION-DEBUG-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\GLOBALIZATION\KYLOG-GLOBALIZATION-DEBUG-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="CCIDATASCHEDULER" xsi:type="File"
				layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}"
				fileName="C:\KYLOG\AOIGUI\CCIGUI\DATASCHEDULER\KYLOG-DATASCHEDULER-${date:yyyyMMdd}.LOG"
				keepFileOpen="false"
				archiveNumbering="Sequence"
				archiveAboveSize="5242880"
				archiveFileName="C:\KYLOG\AOIGUI\CCIGUI\DATASCHEDULER\KYLOG-DATASCHEDULER-${date:yyyyMMdd}_{#}.LOG"
				encoding="utf-8" />

    <target name="console" xsi:type="Console" layout="[ ${level:uppercase=true} ] ${message}${exception:format=tostring}" />

    <target name="Network" xsi:type="Network" address="udp://127.0.0.2:4000" layout="&lt;${date:format=yyyyMMdd HH\:mm\:ss\:fff}&gt; ${message} ${exception:format=tostring}" encoding="utf-8"/>
  </targets>

  <rules>
    <logger name="CCIInfo" minlevel="Debug" writeTo="CCIINFO,console" />
    <logger name="CCIError" minlevel="Debug" writeTo="CCIERROR,console" />
    <logger name="CCIGUIConnector" minlevel="Debug" writeTo="CCIGUICONNECTOR,console" />
    <logger name="CCIDBConnector" minlevel="Debug" writeTo="CCIDBCONNECTOR,console" />
    <logger name="CCIDebug" minlevel="Debug" writeTo="CCIDEBUG,console" />
    <logger name="Globalization" minlevel="Debug" writeTo="GLOBALIZATION,console" />
    <logger name="CCIDataScheduler" minlevel="Debug" writeTo="CCIDATASCHEDULER,console" />
  </rules>
</nlog>