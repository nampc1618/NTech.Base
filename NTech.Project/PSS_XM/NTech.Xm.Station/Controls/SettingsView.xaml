                                     FontWeight="SemiBold"
                                     Foreground="White"
                                     Text="{Binding PrinterViewModel.PRINTERSelected.WidthMsg, Mode=TwoWay}"
                                     VerticalAlignment="Center"
                                     TextAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Grid.Row="3"
                                   LastChildFill="True">
                            <TextBlock Margin="15 0 0 0"
                                       Text="Thời gian trễ:"
                                       FontSize="16"
                                       Foreground="White"
                                       FontWeight="SemiBold"
                                       VerticalAlignment="Center"/>
                            <TextBox Margin="70 0 0 0"
                                     FontSize="16"
                                     FontWeight="SemiBold"
                                     Foreground="White"
                                     Text="{Binding PrinterViewModel.PRINTERSelected.Delay,Mode=TwoWay}"
                                     VerticalAlignment="Center"
                                     TextAlignment="Center"/>
                        </DockPanel>
                        <DockPanel Grid.Row="4"
                                   LastChildFill="True">
                            <TextBlock Margin="15 0 0 0"
                                       Text="Hướng bản tin:"
                                       FontSize="16"
                                       Foreground="White"
                                       FontWeight="SemiBold"
                                       VerticalAlignment="Center" />
                            <ComboBox Name="cbbFieldOrientation"
                                      FontSize="18"
                                      FontWeight="SemiBold"
                                      Margin="55 0 0 0"
                                      Style="{DynamicResource NTech.Wpf.ComboBox}"
                                      VerticalAlignment="Center"
                                      MinWidth="180"
                                      MaxWidth="350"
                                      Height="30"
                                      ItemsSource="{Binding PrinterViewModel.FieldOrientations}"
                                      SelectedItem="{Binding PrinterViewModel.PRINTERSelected.StrFieldOrientation, Mode=TwoWay}"
                                      ItemContainerStyle="{DynamicResource NTech.Wpf.ComboBoxItem}">
                            </ComboBox>
                        </DockPanel>
                    </Grid>
                    <Grid Grid.Column="1">
                        <Button Content="SAVE"
                                Style="{DynamicResource NTech.Wpf.ButtonRoundBound}"
                                Margin="5"
                                Foreground="White"
                                Command="{Binding SaveMsgParamCmd}">
                            <Button.Background>
                                <LinearGradientBrush EndPoint="0.5,1" StartPoint="0.5,0">
                                    <GradientStop Color="#FF0C3771"/>
                                    <GradientStop Color="#FF0D73F9" Offset="1"/>
                                </LinearGradientBrush>
                            </Button.Background>
                        </Button>
                    </Grid>
                </Grid>
            </Border>

        </Grid>
        <Grid Grid.Row="2">
            <Border BorderBrush="LightGray"
                    BorderThickness="1"
                    CornerRadius="5"
                    Margin="2">
                <Grid>
                    <Grid.RowDefinitions>
                        <RowDefinition />
                        <RowDefinition />
                        <RowDefinition />
                    </Grid.RowDefinitions>
                    <StackPanel Orientation="Horizontal">
                        <TextBlock Margin="20 5 5 5"
                                   Text="Dùng PLC: "
                                   FontSize="16"
                                   Foreground="White"
                                   FontWeight="SemiBold"
                                   VerticalAlignment="Center" />
                        <CheckBox Margin="5"
                                  Height="30"
                                  Width="30"
                                  Style="{DynamicResource NTech.Wpf.CheckBox}"
                                  IsChecked="{Binding IsUsePLC, Mode=TwoWay}"/>
                    </StackPanel>
                </Grid>
            </Border>
        </Grid>
    </Grid>
</UserControl>
